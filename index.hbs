{{!< default}}

{{!-- Navbar partial --}}
{{> "navbar"}}
{{> "search"}}

<main class="i-home-cards i-home-section">
    <h2 class="i-home-cards-title">{{#tag}}{{name}}{{/tag}}</h2>
    <div class="i-home-cards-grid">
            {{#foreach posts from="1" to="6"}}
                {{> "picture-card"}}
            {{/foreach}}
    </div>
    
    
    
    <div class="i-home-cards-grid">
            {{#foreach posts from="7" to="12"}}
                {{> "line-card"}}
        {{/foreach}}
    </div>
</main>

{{pagination}}

